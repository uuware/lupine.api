{
  "name": "lupine.api",
  "version": "1.0.1",
  "license": "MIT",
  "author": "uuware.com",
  "homepage": "https://uuware.com/",
  "description": "LupineApi is a fast, lightweight, and flexible node.js based server, working with LupineJs to provide SSR and modern JavaScript features for web applications and APIs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/uuware/lupine.api.git"
  },
  "main": "src/index.ts",
  "source": "src/index.ts",
  "types": "src/index.ts",
  "engines": {
    "node": ">= 20"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "umd": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./admin": {
      "types": "./admin/index.ts",
      "import": "./admin/index.ts",
      "require": "./admin/index.ts"
    },
    "./dev": {
      "require": "./dev/index.js"
    }
  },
  "keywords": [
    "backend-end",
    "lightweight"
  ],
  "scripts": {
    "note": "echo 'build is not needed as the typescript code is supposed to be referred directly from other projects'",
    "build": "tsc"
  },
  "dependencies": {
    "lupine.js": "^1.0.0",
    "better-sqlite3": "^11.9.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5"
  }
}
